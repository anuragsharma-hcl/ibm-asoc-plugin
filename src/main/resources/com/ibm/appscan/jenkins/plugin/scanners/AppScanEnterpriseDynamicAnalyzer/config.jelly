        <f:entry title="${%label.login.management}" field="loginTypeOptions" inline="true">
            
            <f:radioBlock name="loginType" title="${%label.recorded}" >
                <st:nbsp/>
                <f:entry title="Upload Traffic File" field="trafficType">
            
                    <f:entry title="Location" id="ifRecorded" field="trafficFile">
                        <f:textbox />
                    </f:entry>
                </f:entry>
            </f:radioBlock>
            <f:radioBlock name="loginType" title="${%label.automatic}">
                <f:entry title="User Name" help="...">
                    <f:textbox field="accessId" />
                </f:entry>
                <f:entry title="Password" help="...">
                    <f:password field="secretKey" />
                </f:entry>
            </f:radioBlock>
            <br/>
            <f:radioBlock name="loginType" title="${%label.none}"/>
        </f:entry>
        
        <f:entry title="Scan Mode" field="scanType" inline="true">
                            <f:radioBlock name="scanType"  title="Full Scan"/>
                            <f:radioBlock name="scanType"  title="Test Only"/>
                            <f:radioBlock name="scanType"  title="Explore Only"/>
        </f:entry>
        
        <f:entry title="Upload Traffic as explore data" field="exploreData">
		<f:textbox />
	</f:entry>
	<f:entry title="Upload Traffic for multi-step" field="multiStep">
		<f:textbox />
	</f:entry>
        <f:optionalBlock title="${%label.additional.options}" field="hasOptions" inline="true">

		<f:entry title="${%label.ase.agent}" field="aseAgent">
			<f:select />
		</f:entry>
						
	</f:optionalBlock>

</j:jelly>
