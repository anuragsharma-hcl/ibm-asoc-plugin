	<f:entry title="${%label.application}" field="application">
		<f:select />
	</f:entry>
	<f:entry title="${%label.starting.url}" field="target">
		<f:textbox />
	</f:entry>
        
        <f:entry title="${%label.ase.folder}" field="folderId">
		<f:select />
	</f:entry>
        
        <f:entry title="${%label.ase.test.policies}" field="aseTestPolicies">
		<f:select />
	</f:entry>
        
        <f:entry title="ASE Templates" field="aseTemplate">
		<f:select />
	</f:entry>
        
